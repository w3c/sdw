  <owl:Ontology rdf:about="http://www.w3.org/ns/ssn/">
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <dc:title xml:lang="en">Semantic Sensor Network Ontology</dc:title>
    <dc:description xml:lang="en">This ontology describes sensors, actuators and observations, and related concepts. It does not describe domain concepts, time, locations, etc. these are intended to be included from other ontologies via OWL imports.</dc:description>
    <dc:creator>
      <foaf:Agent>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
      </foaf:Agent>
    </dc:creator>

    <dc:rights>Copyright 2017 W3C/OGC.</dc:rights>
    <dc:license rdf:resource="http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document"/>
    <dc:license rdf:resource="http://www.opengeospatial.org/ogc/Software"/>
    <owl:versionInfo xml:lang="en">New modular version of the SSN ontology. 

This ontology was originally developed in 2009-2011 by the W3C Semantic Sensor Networks Incubator Group (SSN-XG). For more information on the group's activities see: http://www.w3.org/2005/Incubator/ssn/. The ontology was revised and modularized in 2015-2017 by the W3C/OGC Spatial Data on the Web Working Group, see: https://www.w3.org/2015/spatial/wiki/Semantic_Sensor_Network_Ontology. 

In particular, (a) the scope is extended to include actuation and sampling; (b) the core concepts and properties are factored out into the SOSA ontology. The SSN ontology imports SOSA and adds formal axiomatization consistent with the text definitions in SOSA, and adds classes and properties to accommodate the scope of the original SSN ontology. </owl:versionInfo>
    <rdfs:comment xml:lang="en">Please report any errors to the W3C Spatial Data on the Web Working Group via the SDW WG Public List public-sdw-wg@w3.org</rdfs:comment>
    <rdfs:seeAlso rdf:resource="https://www.w3.org/2015/spatial/wiki/Semantic_Sensor_Network_Ontology"/>
    <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-04-17</dc:created>
    <ns0:preferredNamespacePrefix>ssn</ns0:preferredNamespacePrefix>
    <ns0:preferredNamespaceUri>http://www.w3.org/ns/ssn/</ns0:preferredNamespaceUri>
    <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
  </owl:Ontology>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/FeatureOfInterest">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/Property"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="http://www.w3.org/ns/sosa/hasSample">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </owl:InverseFunctionalProperty>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <owl:Class rdf:about="http://www.w3.org/ns/ssn/Property">
    <rdfs:label xml:lang="en">Property</rdfs:label>
    <skos:definition xml:lang="en">A quality of a FeatureOfInterest. An aspect of an entity that is intrinsic to and cannot exist without the entity.</skos:definition>
    <rdfs:comment xml:lang="en">A quality of a FeatureOfInterest. An aspect of an entity that is intrinsic to and cannot exist without the entity.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:label xml:lang="en">has property</rdfs:label>
    <skos:definition xml:lang="en">Relation between a FeatureOfInterest and a Property of that feature.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a FeatureOfInterest and a Property of that feature.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/isPropertyOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">is property of</rdfs:label>
    <skos:definition xml:lang="en">Relation between a Property and the FeatureOfInterest it belongs to.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a Property and the FeatureOfInterest it belongs to.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/ObservableProperty">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isObservedBy">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description>
            <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/observedProperty"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description>
            <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/isProxyFor"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/Stimulus"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/ActuatableProperty">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isActedOnBy">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Sample">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isSamplingResultOf"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isSampleOf"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isSampleOf"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isSamplingResultOf"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <owl:FunctionalProperty rdf:about="http://www.w3.org/ns/sosa/isSampleOf">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </owl:FunctionalProperty>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Platform">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/hosts">
            <owl:propertyChainAxiom>
              <rdf:Description>
                <rdf:first rdf:resource="http://www.w3.org/ns/ssn/hasDeployment"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://www.w3.org/ns/ssn/deployedSystem"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:propertyChainAxiom>

            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/System"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/inDeployment"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/Deployment"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/isHostedBy">
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Procedure">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/hasInput"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.w3.org/ns/ssn/Input">
            <rdfs:label xml:lang="en">Input</rdfs:label>
            <skos:definition xml:lang="en">Any information that is provided to a Procedure for its use. [MMI OntDev]</skos:definition>
            <rdfs:comment xml:lang="en">Any information that is provided to a Procedure for its use. [MMI OntDev]</rdfs:comment>
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty>
                  <rdf:Description>
                    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/hasInput"/>
                  </rdf:Description>
                </owl:onProperty>

                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
              </owl:Restriction>
            </rdfs:subClassOf>

            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty>
                  <rdf:Description>
                    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/hasInput"/>
                  </rdf:Description>
                </owl:onProperty>

                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
              </owl:Restriction>
            </rdfs:subClassOf>

            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
          </owl:Class>
        </owl:allValuesFrom>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/hasOutput"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.w3.org/ns/ssn/Output">
            <rdfs:label xml:lang="en">Output</rdfs:label>
            <skos:definition xml:lang="en">Any information that is reported from a Procedure. [MMI OntDev]</skos:definition>
            <rdfs:comment xml:lang="en">Any information that is reported from a Procedure. [MMI OntDev]</rdfs:comment>
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty>
                  <rdf:Description>
                    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/hasOutput"/>
                  </rdf:Description>
                </owl:onProperty>

                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
              </owl:Restriction>
            </rdfs:subClassOf>

            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty>
                  <rdf:Description>
                    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/hasOutput"/>
                  </rdf:Description>
                </owl:onProperty>

                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
              </owl:Restriction>
            </rdfs:subClassOf>

            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
          </owl:Class>
        </owl:allValuesFrom>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/implementedBy"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/System"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasInput">
    <rdfs:label xml:lang="en">has input</rdfs:label>
    <skos:definition xml:lang="en">Relation between a Procedure and an Input to it.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a Procedure and an Input to it.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasOutput">
    <rdfs:label xml:lang="en">has output</rdfs:label>
    <skos:definition xml:lang="en">Relation between a Procedure and an Output of it.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a Procedure and an Output of it.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/implements">
    <rdfs:label xml:lang="en">implements</rdfs:label>
    <skos:definition xml:lang="en">Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method).</skos:definition>
    <rdfs:comment xml:lang="en">Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method).</rdfs:comment>
    <skos:example xml:lang="en">For example, the relationship between a sensor and the scientific measuring Procedure via which it senses.</skos:example>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/implementedBy"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/implementedBy">
    <rdfs:label xml:lang="en">implemented by</rdfs:label>
    <skos:definition xml:lang="en">Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way.</skos:definition>
    <rdfs:comment xml:lang="en">Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way.</rdfs:comment>
    <skos:example xml:lang="en">For example, the relationship between a scientific measuring Procedure and a sensor that senses via that Procedure.</skos:example>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/implements"/>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
  </owl:ObjectProperty>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Sensor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/implements"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/observes">
            <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeObservation">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/detects"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/ssn/Stimulus"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Actuator">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/implements"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/ActuatableProperty"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeActuation">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Sampler">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/implements"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/madeSampling">
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
        </owl:onProperty>

        <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
