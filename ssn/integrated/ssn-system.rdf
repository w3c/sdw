     xmlns:sosa="http://www.w3.org/ns/sosa/">
    <owl:Ontology rdf:about="http://www.w3.org/ns/ssn/systems/">
        <owl:imports rdf:resource="http://www.w3.org/ns/ssn/"/>
        <rdfs:seeAlso rdf:resource="https://www.w3.org/2015/spatial/wiki/Semantic_Sensor_Network_Ontology"/>
        <dcterms:license rdf:resource="http://www.opengeospatial.org/ogc/Software"/>
        <dcterms:license rdf:resource="http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document"/>
        <vann:preferredNamespaceUri>http://www.w3.org/ns/ssn/systems/</vann:preferredNamespaceUri>
        <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-05-14</dcterms:created>
        <dcterms:description xml:lang="en">This ontology describes system capabilities, operating ranges, and survival ranges.</dcterms:description>
        <dcterms:creator>
            <rdf:Description>
                <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
                <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
            </rdf:Description>
        </dcterms:creator>
        <dcterms:rights>Copyright 2017 W3C/OGC.</dcterms:rights>
        <dcterms:title xml:lang="en">System capabilities, operating ranges, and survival ranges ontology</dcterms:title>
        <vann:preferredNamespacePrefix>ssn-system</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">Please report any errors to the W3C Spatial Data on the Web Working Group via the SDW WG Public List public-sdw-wg@w3.org</rdfs:comment>
    </owl:Ontology>
    


