    <!-- http://www.w3.org/ns/ssn/systems/hasOperatingProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasOperatingProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from an OperatingRange of a System to a OperatingPropery describing the operating range of the System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has operating property</rdfs:label>
        <skos:definition xml:lang="en">Relation from an OperatingRange of a System to a OperatingPropery describing the operating range of the System.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/hasOperatingRange -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasOperatingRange">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from a System to an OperatingRange describing the normal operating environment of the System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has operating range</rdfs:label>
        <skos:definition xml:lang="en">Relation from a System to an OperatingRange describing the normal operating environment of the System.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/hasSurvivalProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSurvivalProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from a SurvivalRange of a System to a SurvivalProperty describing the survival range of the System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has survival property</rdfs:label>
        <skos:definition xml:lang="en">Relation from a SurvivalRange of a System to a SurvivalProperty describing the survival range of the System.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/hasSurvivalRange -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSurvivalRange">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from a System to a SurvivalRange.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has survival range</rdfs:label>
        <skos:definition xml:lang="en">Relation from a System to a SurvivalRange.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/hasSystemCapability -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSystemCapability">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from a System to a SystemCapability describing the capabilities of the System under certain Conditions.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has system capability</rdfs:label>
        <skos:definition xml:lang="en">Relation from a System to a SystemCapability describing the capabilities of the System under certain Conditions.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/hasSystemProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSystemProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Relation from an SystemCapability of a System to a SystemProperty describing the capabilities of the System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">has system property</rdfs:label>
        <skos:definition xml:lang="en">Relation from an SystemCapability of a System to a SystemProperty describing the capabilities of the System.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/inCondition -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/inCondition">
        <rdfs:comment xml:lang="en">Describes the prevailing environmental conditions for SystemCapabilites, OperatingRanges and SurvivalRanges.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/systems/"/>
        <rdfs:label xml:lang="en">in condition</rdfs:label>
        <skos:definition xml:lang="en">Describes the prevailing environmental conditions for SystemCapabilites, OperatingRanges and SurvivalRanges.</skos:definition>
        <skos:example xml:lang="en">Used for example to say that a sensor has a particular accuracy in particular conditions.</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/systems/qualityOfObservation -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/qualityOfObservation">
        <rdfs:comment xml:lang="en">Relation linking an Observation to the adjudged quality of the Result. This is complimentary to the SystemCapability information recorded for the Sensor that made the Observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
        <rdfs:label xml:lang="en">quality of observation</rdfs:label>
        <skos:definition xml:lang="en">Relation linking an Observation to the adjudged quality of the Result. This is complimentary to the SystemCapability information recorded for the Sensor that made the Observation.</skos:definition>
    </owl:ObjectProperty>
    


