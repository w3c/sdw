              <owl:DatatypeProperty rdf:ID="nominalPosition"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:cardinality>
          </owl:Restriction>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TemporalPosition"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Time position</rdfs:label>
    <rdfs:comment xml:lang="en">A temporal position described using either a (nominal) value from an ordinal reference system, or a (numeric) value in a temporal coordinate system. </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#TemporalPosition">
